<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NasaAstronomy</servlet-name>
        <servlet-class>com.apijava.controller.NasaAstronomy</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarvelComic</servlet-name>
        <servlet-class>com.apijava.controller.MarvelComic</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edamam</servlet-name>
        <servlet-class>com.apijava.controller.Edamam</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edamams</servlet-name>
        <servlet-class>com.apijava.controller.Edamams</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarvelId</servlet-name>
        <servlet-class>com.apijava.controller.MarvelId</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NasaAstronomy</servlet-name>
        <url-pattern>/NasaAstronomy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarvelComic</servlet-name>
        <url-pattern>/MarvelComic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edamam</servlet-name>
        <url-pattern>/Edamam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edamams</servlet-name>
        <url-pattern>/Edamams</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edamams</servlet-name>
        <url-pattern>/Edamams/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarvelId</servlet-name>
        <url-pattern>/MarvelId</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
